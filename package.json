{
  "name": "neocode-ia-diabetics",
  "version": "1.0.0",
  "description": "Predição de Diabetes usando Redes Neurais Profundas",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "prettier:write": "prettier --write .",
    "lint": "eslint .",
    "prepare": "husky install",
    "commit": "git add . && npx cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neocoode/node-ia-tensorflow-deep-neural-network-diabetic.git"
  },
  "author": "Neocoode",
  "license": "LicenseRef-LICENSE",
  "keywords": [],
  "engines": {
    "node": ">=16.0.0 <19.0.0",
    "npm": ">=10.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@commitlint/prompt-cli": "^17.0.3",
    "@j-ulrich/release-it-regex-bumper": "^5.1.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.0",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "webpack": "^5.74.0"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.21.0",
    "tsconfig-paths": "^4.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --config eslint.config.js --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run commit"
    }
  }
}
